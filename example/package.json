{
  "private": true,
  "name": "example",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "nest build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "nest-boot": "nest-boot"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.1.2",
    "@mikro-orm/nestjs": "^5.0.1",
    "@mikro-orm/postgresql": "^5.1.2",
    "@mikro-orm/reflection": "^5.1.2",
    "@nest-boot/auth": "1.2.11",
    "@nest-boot/command": "^1.0.0",
    "@nest-boot/common": "1.4.1",
    "@nest-boot/database": "^1.0.0",
    "@nest-boot/graphql": "^1.0.0",
    "@nest-boot/health-check": "^1.0.0",
    "@nest-boot/i18next": "^1.0.0",
    "@nest-boot/mailer": "^1.0.0",
    "@nest-boot/queue": "^1.0.0",
    "@nest-boot/redis": "^1.0.0",
    "@nest-boot/search": "^1.0.0",
    "@nest-boot/search-engine-meilisearch": "^1.0.0",
    "@nest-boot/validator": "^1.0.0",
    "@nestjs/apollo": "^10.0.9",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.4.4",
    "@nestjs/graphql": "^10.0.9",
    "@nestjs/platform-express": "^8.4.4",
    "apollo-server-core": "^3.6.7",
    "apollo-server-express": "^3.6.7",
    "argon2": "^0.28.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.17.3",
    "graphql": "^15.8.0",
    "meilisearch": "^0.25.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.1.2",
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.12",
    "jest": "^27.5.1",
    "pino-pretty": "^7.6.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "volta": {
    "extends": "../package.json"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
