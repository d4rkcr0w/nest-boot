    "@semantic-release/git": "^10.0.1",
    "eslint": "^8.7.0",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "optionalDependencies": {
    "semantic-release": "*"
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.17"
  }
