{
  "private": true,
  "name": "@nest-boot/example",
  "version": "0.0.0-development",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nest build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "nest-boot": "nest-boot"
  },
  "dependencies": {
    "@nest-boot/auth": "workspace:^",
    "@nest-boot/command": "workspace:^",
    "@nest-boot/common": "workspace:^",
    "@nest-boot/database": "workspace:^",
    "@nest-boot/graphql": "workspace:^",
    "@nest-boot/health-check": "workspace:^",
    "@nest-boot/i18next": "workspace:^",
    "@nest-boot/logger": "workspace:^",
    "@nest-boot/mailer": "workspace:^",
    "@nest-boot/queue": "workspace:^",
    "@nest-boot/redis": "workspace:^",
    "@nest-boot/search": "workspace:^",
    "@nest-boot/search-engine-meilisearch": "workspace:^",
    "@nest-boot/tenant": "workspace:^",
    "@nest-boot/validator": "workspace:^",
    "@nestjs/apollo": "^9.2.7",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/graphql": "^9.2.7",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "argon2": "^0.28.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "marked": "^4.0.12",
    "meilisearch": "^0.24.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "source-map-support": "^0.5.21",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/marked": "^4.0.2",
    "@types/node": "^17.0.14",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.11",
    "jest": "^27.4.7",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
