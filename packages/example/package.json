{
  "private": true,
  "name": "@nest-boot/example",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "nest build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nest-boot/auth": "workspace:^",
    "@nest-boot/command": "workspace:^",
    "@nest-boot/common": "workspace:^",
    "@nest-boot/database": "workspace:^",
    "@nest-boot/graphql": "workspace:^",
    "@nest-boot/health-check": "workspace:^",
    "@nest-boot/i18next": "workspace:^",
    "@nest-boot/logger": "workspace:^",
    "@nest-boot/mailer": "workspace:^",
    "@nest-boot/queue": "workspace:^",
    "@nest-boot/redis": "workspace:^",
    "@nest-boot/search": "workspace:^",
    "@nest-boot/validator": "workspace:^",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^9.0.0",
    "@nestjs/platform-express": "^8.0.6",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.3.0",
    "argon2": "^0.27.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.0",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "meilisearch": "^0.20.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/express": "^4.17.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.14.6",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-superboost": "^1.2.9",
    "jest": "^27.4.7",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
