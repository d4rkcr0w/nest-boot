import { Command, Positional } from "@nest-boot/command";
import { Injectable } from "@nestjs/common";

@Injectable()
export class {{ name | camelize }}Command {
  @Command({
    command: "{{ name | underscore | dasherize }} <value>",
    describe: "命令描述",
  })
  async handle(
    @Positional({
      name: "value",
      describe: "值",
      type: "string",
    })
    value: string
  ): Promise<void> {
    // eslint-disable-next-line no-console
    console.log("value:", value);
  }
}
